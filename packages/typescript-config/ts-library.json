{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Typescript Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2020"],
    "module": "ESNext",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "target": "es2020"
  }
}
